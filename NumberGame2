import java.util.Scanner;

public class MyScanner{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int max = 100;
		int min = 1;
		System.out.println("What is a number from " + min + " to " + max);
		int goal = scanner.nextInt();
		int guess = (max+min)/2;
		boolean guessWrong = true;
		while (guessWrong){
		
		if (guess>goal) {
		System.out.println(guess);
		max = (max+min)/2;
		guess = (max+min)/2;
		guessWrong = true;
		}
		else if(guess<goal) {
		System.out.println(guess);
		min = (max+min)/2;
		guess = (max+min)/2;
		guessWrong = true;
		}
		else if(guess==goal) {
		System.out.println("The goal is: " + guess);
		guessWrong = false;
		}
		else {
		System.out.println("You cheated");
		}
		}
		System.out.println("Your number is: " + guess);
	}
}
