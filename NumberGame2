import java.util.Scanner;

public class MyScanner{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int max = 100;
		int min = 1;
		System.out.println("What is a number from " + min + " to " + max);
		int goal = scanner.nextInt();
		int guess = (int)Math.ceil((max+min)/2);
		boolean guessWrong = true;
		
		while (guessWrong) {
		guess = (int)Math.ceil((max+min)/2);

	//	System.out.println(guess);
		if (guess==goal) {
		System.out.println("Your number is: " + guess);//min++;//(int)Math.ceil((max+min)/2);
		
		break;
		}
		else if (guess>goal) {
		max = guess;//max--;//(int)Math.ceil((max+min)/2);
		break;
		}
		else if (guess<goal) {
		min = guess;
		break;
		}
		//guessWrong = true;
		}

	
	}
}
