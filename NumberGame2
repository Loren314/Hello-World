			guessWrong = false;
			} else {
			System.out.println("You cheated");
			}
		}*/
	}
}

















import java.util.Scanner;

public class MyScanner{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int max = 100;
		int min = 1;
		System.out.println("What is a number from " + min + " to " + max);
		int goal = scanner.nextInt();
		int guess = (int)Math.ceil((max+min)/2);
		boolean guessWrong = true;
		
		while (guess!=goal) {
		guess = (int)Math.ceil((max+min)/2);

	//	System.out.println(guess);
		if (guess>goal) {
		min = guess;//min++;//(int)Math.ceil((max+min)/2);
		
		break;
		}
		else {
		max = guess;//max--;//(int)Math.ceil((max+min)/2);
		break;
		}
		//guessWrong = true;
		}
	//	while (guess<goal) {
	//	System.out.println(guess);
	//	max = (max+min)/2;
	//	guess = (max+min)/2;
	//	guessWrong = true;
	//	}
		while (guessWrong = true) {
		System.out.println("Your number is: " + guess);
		guessWrong = false;
		}
	/*	while (guessWrong=true){
		
